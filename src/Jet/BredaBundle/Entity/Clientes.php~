<?php

namespace Jet\BredaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Jet\BredaBundle\Entity\Clientes
 *
 * @ORM\Table()
 * @ORM\Entity
 */
class Clientes
{
 /**
     * @var integer $id
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var text $descipcion
     *
     * @ORM\Column(name="titulo", type="text")
     */
    private $nombre;
}